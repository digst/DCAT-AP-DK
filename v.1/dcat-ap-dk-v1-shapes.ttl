@prefix : <http://data.gov.dk/model/profile/dcat-ap-dk#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix cv: <http://data.europa.eu/m8g/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcatdk: <http://data.gov.dk/model/profile/dcatdk#> .


### UNDER UDVIKLING (developement)

:DatasetShape
    a sh:NodeShape ;
    sh:targetClass dcat:Dataset ;

    sh:property [
      sh:path dc:title;
      sh:languageIn ( "da" ) ;
      sh:name "Titel" ;
      sh:description "Datasættets formelle navn" ;
      sh:minCount 1 ;
    ] ;

    sh:property [
      sh:path dc:description;
      sh:name "Beskrivelse" ;
      sh:description "Beskrivelse af datasættets formål og indhold ";
      sh:languageIn ( "da" ) ;
      sh:minCount 1 ;
    ] ;

    sh:property [
      sh:path dc:language ;
      sh:name "Sprog" ;
      sh:class skos:Concept ;
      sh:nodeKind sh:IRI ;
    ] ;

    sh:property [
      sh:path dc:publisher ;
      sh:name "Udgiver" ;
      sh:node schema:OrganizationShape ;
    ] ;

    sh:property [
      sh:path dcatdk:dataResopnsibleOrganisation ;
      sh:name "Dataansvarlig" ;
      sh:node schema:OrganizationShape ;
    ] ;

    sh:property [
      sh:path dct:accrualPeriodicity ;
      sh:name "Opdateringsfrekvens" ;
      sh:class dct:Frequency ;
      sh:maxCount 1 ;
    ] ;

    sh:property [
      sh:path dc:conformsTo ;
      sh:name "Overholder" ;
      sh:nodeKind sh:IRI ;
    ] ;

    sh:property [
      sh:path dcat:landingPage ;
      sh:name "Kilde" ;
      sh:nodeKind sh:IRI ;
    ] ;

    sh:property [
      sh:path foaf:page ;
      sh:name "Dokumentation" ;
      sh:nodeKind sh:IRI ;
    ] ;

    sh:property [
      sh:path cv:hasLegalResource ;
      sh:name "Hjemmel" ;
      sh:nodeKind sh:IRI ;
    ] ;

    sh:property [
      sh:path dc:spatial ;
      sh:name "Geografisk afgrænsning" ;
    ] ;


    sh:property [
      sh:path dcatdk:paymentRequired ;
      sh:name "Betalingspålagt" ;
      sh:path sh:closed ;
      sh:datatype xsd:boolean ;      
      sh:maxCount 1 ;                 
    ] ;

    sh:property [
      sh:path dcatdk:personalDataCategory ;
      sh:name "Personoplysningskategori" ;   
      sh:nodeKind sh:IRI ;             
    ] ;

    sh:property [
      sh:path dc:subject ;
      sh:name "Forvaltningsopgave" ; 
    ] ;

    sh:property [
      sh:path dcat:theme ;
      sh:nodeKind sh:IRI ;
    ] .


:OrganizationShape 
    a sh:NodeShape ;
    sh:targetClass foaf:Organization ;

    sh:property [
        sh:path foaf:name ;
	sh:languageIn ( "da" ) ;
    ] .


