dcat-ap:Catalog_Shape
  a <http://www.w3.org/ns/shacl#NodeShape> ;
  sh:name "Catalog"@en, "Katalog"@da ;
  sh:description "En organiseret samling af datarelaterede ressourcer i et katalog."@da ;
  sh:targetClass dcat:Catalog ;
  sh:property [
    sh:name "titel"@da ;
    sh:description "datakatalogets titel"@da ;
    sh:minCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path dc:title ;
    sh:severity sh:Violation ;
    sh:group <https:/data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "beskrivelse"@da ;
    sh:description "tekstbaseret beskrivelse af datakatalogets formål og indhold"@da ;
    sh:minCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path dc:description ;
    sh:severity sh:Violation ;
    sh:group <https:/data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "udgiver"@da ;
    sh:description "aktør der har udgivet kataloget"@da ;
    sh:class foaf:Agent ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path dc:publisher ;
    sh:severity sh:Violation ;
    sh:group <https:/data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
