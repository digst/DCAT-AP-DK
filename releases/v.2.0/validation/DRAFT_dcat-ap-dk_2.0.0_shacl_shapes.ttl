    sh:minCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path dc:title ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "description"@en, "beskrivelse"@da ;
    sh:description "This property contains a free-text account of the Dataset. This property can be repeated for parallel language versions of the description."@en, "Tekstbaseret beskrivelse af datasættets formål og indhold. Egenskaben kan gentages for hvert sprog."@da ;
    sh:minCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path dc:description ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "publisher"@en, "udgiver"@da ;
    sh:description "This property refers to the entity (organisation) responsible for making the Dataset available."@en, ""@da ;
    sh:class foaf:Agent ;
    sh:maxCount 1 ;
    sh:path dc:publisher ;
    sh:severity sh:Violation ;
