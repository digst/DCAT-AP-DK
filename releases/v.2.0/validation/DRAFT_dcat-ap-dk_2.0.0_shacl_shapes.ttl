    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Optional>
  ], [
    sh:name "contact point"@en, "kontaktpunkt"@da ;
    sh:description "This property contains contact information that can be used for sending comments about the Dataset."@en, ""@da ;
    sh:class vcard:Kind ;
