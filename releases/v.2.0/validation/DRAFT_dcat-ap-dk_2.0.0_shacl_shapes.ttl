dcat-ap:Catalog_Shape
  a <http://www.w3.org/ns/shacl#NodeShape> ;
  sh:name "Catalog"@en, "Katalog"@da ;
  sh:description "A catalogue or repository that hosts the Datasets being described."@en, "En organiseret samling af datarelaterede ressourcer i et katalog."@da ;
  rdfs:comment "A curated collection of metadata about datasets"@en ;
  sh:targetClass dcat:Catalog ;
  sh:property [
    sh:name "title"@en, "titel"@da ;
    sh:description "This property contains a name given to the Catalogue. This property can be repeated for parallel language versions of the name."@en, "datakatalogets titel"@da ;
    sh:minCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path dc:title ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "description"@en, "beskrivelse"@da ;
    sh:description "This property contains a free-text account of the Catalogue. This property can be repeated for parallel language versions of the description."@en, "tekstbaseret beskrivelse af datakatalogets formål og indhold"@da ;
    sh:minCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path dc:description ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "publisher"@en, "udgiver"@da ;
    sh:description "This property refers to an entity (organisation) responsible for making the Catalogue available. "@en, "aktør der har udgivet kataloget"@da ;
    sh:class foaf:Agent ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path dc:publisher ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "creator"@en, "skaber"@da ;
    sh:description "This property refers to the entity primarily responsible for producing the catalogue"@en, "aktør der har skabt kataloget"@da ;
    sh:class foaf:Agent ;
    sh:maxCount 1 ;
    sh:path dc:creator ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "release date"@en, "udgivelsesdato"@da ;
    sh:description "This property contains the date of formal issuance (e.g., publication) of the Catalogue."@en, "den dato datakataloget først blev udgivet"@da ;
    sh:maxCount 1 ;
    sh:node dcat-ap:DateOrDateTimeDataType_Shape ;
    sh:path dc:issued ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "update/modification date"@en, "seneste opdateringsdato"@da ;
    sh:description "This property contains the most recent date on which the Catalogue was changed or modified."@en, "den dato hvor indholdet i datakataloget seneste er blevet ændret"@da ;
    sh:maxCount 1 ;
    sh:node dcat-ap:DateOrDateTimeDataType_Shape ;
    sh:path dc:modified ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "spatial/geographic"@en, "geografisk område"@da ;
    sh:description "This property refers to a geographical area covered by the Catalogue. "@en, "det geografiske område datakataloget dækker"@da ;
    sh:class dc:Location ;
    sh:path dc:spatial ;
    sh:severity sh:Violation
  ], [
    sh:name "language"@en, "sprog"@da ;
    sh:description "This property refers to a language used in the textual metadata describing titles, descriptions, etc. of the Datasets in the Catalogue. This property can be repeated if the metadata is provided in multiple languages."@en, "det sprog der er anvendt i kataloget"@da ;
    sh:class dc:LinguisticSystem ;
    sh:path dc:language ;
    sh:severity sh:Violation
  ], [
    sh:name "license"@en, "licens"@da ;
    sh:description "This property refers to the licence under which the Catalogue can be used or reused."@en, "den licens under hvilken kataloget kan anvendes eller genbruges"@da ;
    sh:class dc:LicenseDocument ;
    sh:maxCount 1 ;
    sh:path dc:license ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "rights"@en, "adgangsrettigheder"@da ;
    sh:description "This property refers to a statement that specifies rights associated with the Catalogue."@en, "reference til dokument der beskriver adgangsrettigheederne til datasættet"@da ;
    sh:class dc:RightsStatement ;
    sh:maxCount 1 ;
    sh:path dc:rights ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "catalog record"@en, "katalogpost"@da ;
    sh:description "This property refers to a Catalogue Record that is part of the Catalogue"@en, "katalogpost som er en del af kataloget"@da ;
    sh:class dcat:CatalogRecord ;
    sh:path dcat:record ;
    sh:severity sh:Violation
  ], [
    sh:name "themes"@en, "emneklassifikation"@da ;
    sh:description "This property refers to a knowledge organization system used to classify the Catalogue's Datasets."@en, "emneklassifikation der anvendes til kategorisering af datasæt i kataloget"@da ;
    sh:class skos:ConceptScheme ;
    sh:path dcat:themeTaxonomy ;
    sh:severity sh:Violation
  ], [
    sh:name "service"@en, "dataservice"@da ;
    sh:description "This property refers to a site or end-point that is listed in the catalog."@en, ""@da ;
    sh:class dcat:DataService ;
    sh:path dcat:service ;
    sh:severity sh:Violation
  ], [
    sh:name "catalog"@en, "katalog"@da ;
    sh:description "This property refers to a catalog whose contents are of interest in the context of this catalog"@en, ""@da ;
    sh:class dcat:Catalog ;
    sh:path dcat:catalog ;
    sh:severity sh:Violation
  ], [
    sh:name "dataset"@en, "datasæt"@da ;
    sh:description "This property links the Catalogue with a Dataset that is part of the Catalogue."@en, ""@da ;
    sh:class dcat:Dataset ;
    sh:minCount 1 ;
    sh:path dcat:dataset ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "homepage"@en, "websted"@da ;
    sh:description "This property refers to a web page that acts as the main page for the Catalogue."@en, ""@da ;
    sh:class foaf:Document ;
    sh:maxCount 1 ;
    sh:path foaf:homepage ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "has part"@en, "delkatalog"@da ;
    sh:description "This property refers to a related Catalogue that is part of the described Catalogue"@en, ""@da ;
    sh:class dcat:Catalog ;
    sh:path dc:hasPart ;
    sh:severity sh:Violation
  ], [
    sh:name "is part of"@en, "del af katalog"@da ;
    sh:description "This property refers to a related Catalogue in which the described Catalogue is physically or logically included."@en, ""@da ;
    sh:class dcat:Catalog ;
    sh:maxCount 1 ;
    sh:path dc:isPartOf ;
    sh:severity sh:Violation
  ] .

dcat-ap:CatalogRecord_Shape
  a sh:NodeShape ;
  sh:targetClass dcat:CatalogRecord ;
  sh:name "Catalog Record"@en, "Katalogpost"@da ;
  sh:description "A description of a Dataset’s entry in the Catalogue. "@en, "Post i et datakatalog der beskriver registreringen af et enkelt datasæt eller en dataservice."@da ;
  sh:property [
    sh:name "primary topic"@en, "genstand for registrering"@da ;
    sh:description "This property links the Catalogue Record to the Dataset, Data service or Catalog described in the record."@en, ""@da ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:node dcat-ap:DcatResource_Shape ;
    sh:path foaf:primaryTopic ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "update/ modification date"@en, "seneste ændringsdato"@da ;
    sh:description "This property contains the most recent date on which the Catalogue entry was changed or modified."@en, ""@da ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path dc:modified ;
    sh:severity sh:Violation ;
    sh:shape dcat-ap:DateOrDateTimeDataType_Shape ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk#Mandatory>
  ], [
    sh:name "listing date"@en, "udgivelsesdato"@da ;
    sh:description "This property contains the date on which the description of the Dataset was included in the Catalogue."@en, ""@da ;
    sh:maxCount 1 ;
    sh:node dcat-ap:DateOrDateTimeDataType_Shape ;
    sh:path dc:issued ;
    sh:severity sh:Violation
  ], [
    sh:name "title"@en, "titel"@da ;
    sh:description "This property contains a name given to the Catalogue Record. This property can be repeated for parallel language versions of the name."@en, ""@da ;
    sh:nodeKind sh:Literal ;
    sh:path dc:title ;
    sh:severity sh:Violation
  ], [
    sh:name "description"@en, "beskrivelse"@da ;
    sh:description "This property contains a free-text account of the record. This property can be repeated for parallel language versions of the description."@en, ""@da ;
    sh:nodeKind sh:Literal ;
    sh:path dc:description ;
    sh:severity sh:Violation
  ], [
    sh:name "change type"@en, "status"@da ;
    sh:description "This property refers to the type of the latest revision of a Dataset's entry in the Catalogue. "@en, ""@da ;
    sh:class skos:Concept ;
    sh:maxCount 1 ;
    sh:path <http://www.w3.org/ns/adms#status> ;
    sh:severity sh:Violation
  ], [
    sh:name "language"@en, "sprog"@da ;
    sh:description "This property refers to a language used in the textual metadata describing titles, descriptions, etc. of the Dataset. This property can be repeated if the metadata is provided in multiple languages."@en, ""@da ;
    sh:class dc:LinguisticSystem ;
    sh:path dc:language ;
    sh:severity sh:Violation
  ], [
    sh:name "source metadata"@en, "kilde"@da ;
    sh:description "This property refers to the original metadata that was used in creating metadata for the Dataset"@en, ""@da ;
    sh:class dcat:CatalogRecord ;
    sh:maxCount 1 ;
    sh:path dc:source ;
    sh:severity sh:Violation
  ], [
    sh:name "application profile"@en, "overenstemmelse"@da ;
    sh:description "This property refers to an Application Profile that the Dataset’s metadata conforms to"@en, ""@da ;
    sh:class dc:Standard ;
    sh:maxCount 1 ;
    sh:path dc:conformsTo ;
    sh:severity sh:Violation
  ] .

dcat-ap:Dataset_Shape
  a sh:NodeShape ;
  sh:targetClass dcat:Dataset ;
  sh:name "Dataset"@en, "Datasæt"@da ;
  sh:description "A conceptual entity that represents the information published. "@en, "En samling a data, udgivet eller organiseret af en enkelt kilde og som der er adgang til i en eller flere repræsentationer."@da ;
  sh:property [
    sh:name "identifikator"@da, "identifier"@en ;
    sh:description "This property contains the main identifier for the Dataset, e.g. the URI or other unique identifier in the context of the Catalogue."@en, "Global identifikator for datasættet, for eksempel en URI eller anden identifikator som er stabil og globalt unik"@da ;
