    sh:class <http://data.europa.eu/eli/ontology#LegalResource> ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "version"@en, "versionsnummer"@da ;
    sh:description "This property contains a version number or other version designation of the Dataset."@en, "Denne egenskab angiver en versionsnummer eller anden versionsangivelse af datasættet."@da ;
    rdfs:comment "The annotation property that provides version information for an ontology or another OWL construct."@en, "Annotationsegenskab som angiver versioninformation for en ontologi eller et andet OWL-element."@da ;
    sh:maxCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:path owl:versionInfo ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "version notes"@en, "versionsnoter"@da ;
    sh:description "This property contains a description of the differences between this version and a previous version of the Dataset. This property can be repeated for parallel language versions of the version notes."@en, "Denne egenskab angiver en beskrivelse af forskellene mellem den aktuelle version og forrige version af datasættet. Denne egenskab kan gentages for hver sprogvariant af versionsnoterne."@da ;
    rdfs:comment "A description of changes between this version and the previous version of the Asset"@en, "En beskrivelse af ændringer indført mellem denne version og forrige version af aktivet"@da ;
    sh:nodeKind sh:Literal ;
    sh:path <http://www.w3.org/ns/adms#versionNotes> ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "has version"@en, "har version"@da ;
    sh:description "This property refers to a related Dataset that is a version, edition, or adaptation of the described Dataset."@en, "Denne egenskab angiver et relateret datasæt som er en version, udgave eller bearbejdelse af et aktuelle datasæt."@da ;
    rdfs:comment "A related resource that is a version, edition, or adaptation of the described resource."@en, "En relateret ressource som er en version, udgave eller tilpasning af den beskrevne ressource."@da ;
    sh:class dcat:Dataset ;
    sh:path dct:hasVersion ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "is version of"@en, "er version af"@da ;
    sh:description "This property refers to a related Dataset of which the described Dataset is a version, edition, or adaptation."@en, "Denne egenskab angiver et relateret datasæt som det aktuelle datasæt er en version, udgave eller bearbejdelse af."@da ;
    rdfs:comment "A related resource of which the described resource is a version, edition, or adaptation."@en, "En relateret ressource som den beskrevne ressource er en version, udgave eller tilpasning af."@da ;
    sh:class dcat:Dataset ;
    sh:path dct:isVersionOf ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "provenance"@en, "proveniensudsagn"@da ;
    sh:description "This property contains a statement about the lineage of a Dataset."@en, "Denne egenskab angiver udsagn omkring datasættets proveniens i form af udsagn om eventuelle aktørskift som er centrale for autenciteten, integriteten og fortolkningen af datasættet."@da ;
    rdfs:comment "A statement of any changes in ownership and custody of the resource since its creation that are significant for its authenticity, integrity, and interpretation."@en, "Udsagn omkring datasættets proveniens i form af udsagn om eventuelle aktørskift som er centrale for autenciteten, integriteten og fortolkningen af ressourcen."@da ;
    sh:class dct:ProvenanceStatement ;
    sh:path dct:provenance ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "related resource"@en, "relateret ressource"@da ;
    sh:description "This property refers to a related resource."@en, "Denne egenskab angiver en relateret ressource"@da ;
    rdfs:comment "A related resource."@en, "En relateret ressource."@da ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path dct:relation ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "source dataset"@en, "kildedatasæt"@da ;
    sh:description "This property refers to a related Dataset from which the described Dataset is derived."@en, "Denne egenskab angiver et relateret datasæt hvoraf de aktuelle datasæt er afledt."@da ;
    rdfs:comment "A related resource from which the described resource is derived."@en, "En relateret ressource som den beskrevne ressource er afledt af."@da ;
    sh:class dcat:Dataset ;
    sh:path dct:source ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "is referenced by"@en, "refereres af"@da ;
    sh:description "This property provides a link to a description of a relationship with another resource."@en, "Denne egenskab angiver en reference til en beskrivelse af en relation til en anden ressource."@da ;
    rdfs:comment "A related resource that references, cites, or otherwise points to the described resource."@en, "En relateret ressource som refererer, citerer eller på anden måde peger på den beskrevne ressource."@da ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path dc11:isReferencedBy ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "qualified attribution"@en, "kvalificeret kreditering"@da ;
    sh:description "This property refers to a link to an Agent having some form of responsibility for the resource"@en, "Denne egenskab tilskriver en aktør et bestemt ansvar i forbindelse med datasættet."@da ;
    rdfs:comment "Attribution is the ascribing of an entity to an agent. When an entity e is attributed to agent ag, entity e was generated by some unspecified activity that in turn was associated to agent ag. Thus, this relation is useful when the activity is not known, or irrelevant."@en, "Kreditering er handlingen hvormed en en aktør anerkendes et ansvar for en entitet. "@da ;
    sh:class prov:Attribution ;
    sh:path prov:qualifiedAttribution ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "qualified relation"@en, "kvalificeret relation"@da ;
    sh:description "This property is about a related resource, such as a publication, that references, cites, or otherwise points to the dataset."@en, "Denne egenskab angiver en nærmere klassificeret relation til en anden ressource såsom en publikation der referer, citerer eller på anden på peger på datasættet. Anvendes til at referere til en anden ressource hvor relationens betydning er kendt men ikke matcher en af de standardiserede egenskaber fra Dublin Core eller PROV-O."@da ;
    rdfs:comment "Link to a description of a relationship with another resource."@en, "Reference til en beskrivelse af en relation til en anden ressource."@da ;
    sh:class dcat:Relationship ;
    sh:path dcat:qualifiedRelation ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "was generated by"@en, "blev dannet ved"@da ;
    sh:description "This property refers to an activity that generated, or provides the business context for, the creation of the dataset."@en, "Denne egenskab angiver den aktivitet der dannede eller leverede forretningskonteksten for datasættets skabelse."@da ;
    rdfs:comment "Generation is the completion of production of a new entity by an activity. This entity did not exist before generation and becomes available for usage after this generation."@en, """Generering er det at færdiggøre produktionen af en ny entitet gennem en aktivitet. 
		Generation is the completion of production of a new entity by an activity. This entity did not exist before generation and becomes available for usage after this generation."""@da ;
    sh:class prov:Activity ;
    sh:path prov:wasGeneratedBy ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ] .

<https://data.gov.dk/model/profile/dcat-ap-dk/Distribution_Shape>
  a sh:NodeShape ;
  sh:targetClass dcat:Distribution ;
  sh:name "Distribution"@en, "Distribution"@da ;
  sh:description "A physical embodiment of the Dataset in a particular format."@en, """En specifik repræsentation af datasættet. Et datasæt kan være tilgængelig i mange serialiseringer der kan variere på forskellige vis. Alle distributioner af et givet datasæt indeholder de samme data. Forskelle mellem distributioner skal alene være betinget i distributionens anvendelse af sprog, medietype eller format, systemorganisering, tidslig- og geografisk opløsning, detaljeringsniveau eller profiler (der kan specificere en eller flere af ovenstående). 
	"""@da ;
  rdfs:comment "A specific representation of a dataset. A dataset might be available in multiple serializations that may differ in various ways, including natural language, media-type or format, schematic organization, temporal and spatial resolution, level of detail or profiles (which might specify "@en, "En specifik repræsentation af et datasæt. Et datasæt kan være tilgængelig i mange serialiseringer der kan variere på forskellige vis, herunder sprog, medietype eller format, systemorganisering, tidslig- og geografisk opløsning, detaljeringsniveau eller profiler (der kan specificere en eller flere af ovenstående)."@da ;
  sh:property [
    sh:name "access address"@en, "adgangsadresse"@da ;
    sh:description "This property contains a URL that gives access to a Distribution of the Dataset. The resource at the access URL may contain information about how to get the Dataset. "@en, "Denne egenskab angiver en URL som giver adgang til en distribution af datasættet. "@da ;
    rdfs:comment "A URL of a resource that gives access to a distribution of the dataset. E.g. landing page, feed, SPARQL endpoint. Use for all cases except a simple download link, in which case downloadURL is preferred."@en, "En URL for en ressource som giver adgang til en distribution af datasættet. Fx, destinationsside, feed, SPARQL-endpoint. Anvendes i alle sammenhænge undtagen til angivelse af et simpelt download link hvor anvendelse af egenskaben download-URL foretrækkes."@da ;
    sh:minCount 1 ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path dcat:accessURL ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Mandatory>
  ], [
    sh:name "download URL"@en, "download-URL"@da ;
    sh:description "This property contains a URL that is a direct link to a downloadable file in a given format. "@en, "Denne egenskab angiver en URL der er et direkte link til en fil der kan downloades i et bestemt format."@da ;
    rdfs:comment "The URL of the downloadable file in a given format. E.g. CSV file or RDF file. The format is indicated by the distribution's dct:format and/or dcat:mediaType."@en, "URL til fil der kan downloades i et bestemt format. Fx en CSV-fil eller en RDF-fil. Formatet for distributionen angives ved hjælp af egenskaberne dct:format og/eller dcat:mediaType."@da ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path dcat:downloadURL ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "access service "@en, "adgangstjeneste"@da ;
    sh:description "This property refers to a data service that gives access to the distribution of the dataset"@en, "Denne egenskab angiver en datatjeneste som giver adgang til en distribution af datasættet."@da ;
    rdfs:comment "A site or end-point that gives access to the distribution of the dataset."@en, "Et websted eller endpoint der giver adgang til en distribution af datasættet"@da ;
    sh:class dcat:DataService ;
    sh:path dcat:accessService ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "title"@en, "titel"@da ;
    sh:description "This property contains a name given to the Distribution. This property can be repeated for parallel language versions of the description."@en, "Denne egenskab angiver det eller de ord som navngiver distributionen. Egenskaben kan optræde én gang for hver sprogvariant af titlen."@da ;
    rdfs:comment "A name given to the resource."@en, "Et navn givet til ressourcen."@da ;
    sh:nodeKind sh:Literal ;
    sh:path dct:title ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "description"@en, "beskrivelse"@da ;
    sh:description "This property contains a free-text account of the Distribution. This property can be repeated for parallel language versions of the description."@en, "Denne egenskab giver en tekstbaseret beskrivelse af distributionen. Egenskaben kan optræde én gang for hver sprogvariant af beskrivelsen."@da ;
    rdfs:comment "An account of the resource."@en, "En forklaring af ressourcen."@da ;
    sh:nodeKind sh:Literal ;
    sh:path dct:description ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Recommended>
  ], [
    sh:name "linked schemas"@en, "overholder"@da ;
    sh:description "This property refers to an established schema to which the described Distribution conforms."@en, "Denne egenskab angiver en specifikation (schema) som distributionen er i overensstemmmelse med."@da ;
    rdfs:comment "An established standard to which the described resource conforms."@en, "En estableret standard som den beskrevne ressource er i overensstemelse med."@da ;
    sh:class dct:Standard ;
    sh:path dct:conformsTo ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "release date"@en, "udgivelsesdato"@da ;
    sh:description "This property contains the date of formal issuance (e.g., publication) of the Distribution."@en, "Denne egenskab angiver den dato hvor distributionen først blev formelt udgivet"@da ;
    rdfs:comment "Date of formal issuance of the resource."@en, "Dato for hvornår ressourcen formelt blev udgivet."@en ;
    sh:maxCount 1 ;
    sh:node <https://data.gov.dk/model/profile/dcat-ap-dk/DateOrDateTimeDataType_Shape> ;
    sh:path dct:issued ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "update/ modification date"@en, "seneste ændringsdato"@da ;
    sh:description "This property contains the most recent date on which the Distribution was changed or modified."@en, "Denne egenskab angiver den dato hvor distributionen senest er blevet ændret."@da ;
    rdfs:comment "Date on which the resource was changed."@en, "Dato for hvornår ressourcen blev ændret."@da ;
    sh:maxCount 1 ;
    sh:node <https://data.gov.dk/model/profile/dcat-ap-dk/DateOrDateTimeDataType_Shape> ;
    sh:path dct:modified ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "status"@en, "status"@da ;
    sh:description "This property refers to the maturity of the Distribution. It MUST take one of the values Completed, Deprecated, Under Development, Withdrawn. "@en, "Denne egenskab angiver hvilken tilstand distributionen befinder sig ift. udvikling og ibrugtagning. Til angivelse af status skal følgende klassifikation anvendes: ADMS Status, http://purl.org/adms/status/ (Udfaldsrum: Completed, Deprecated, Under Development, Withdrawn)"@da ;
    rdfs:comment "Links to the status of the Asset or Asset Distribution in the context of a particular workflow process. Since Status is defined using a skos:Concept, that is the defined range for this property."@en, "Relation til den status et aktiv eller et aktivs distribution har i et bestemt workflow. "@da ;
    sh:pattern "^http://purl.org/adms/status/" ;
    sh:flags "i" ;
    sh:maxCount 1 ;
    sh:path <http://www.w3.org/ns/adms#status> ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "availability"@en, "tilgængelighestype"@da ;
    sh:description "This property indicates how long it is planned to keep the Distribution of the Dataset available. "@en, "Denne egenskab indikerer hvor længe det er planlagt at distributionen af datasættet skal være tilgængelig. Til angivelse af tilgængelighedstype skal følgende klassifikation anvendes: Distribution Availability Vocabulary, http://data.europa.eu/r5r/availability/"@da ;
    rdfs:comment "This property indicates how long it is planned to keep the Distribution of the Dataset available. "@en, """Angivelse af hvor længe det er planlagt at distributionen af datasættet skal være tilgængelig.	"""@da ;
    sh:pattern "^http://data.europa.eu/r5r/availability/" ;
    sh:flags "i" ;
    sh:maxCount 1 ;
    sh:path dcat-ap:availability ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Recommended>
  ], [
    sh:name "language"@en, "sprog"@da ;
    sh:description "This property refers to a language used in the Distribution. This property can be repeated if the metadata is provided in multiple languages."@en, "Denne egenskab angiver et sprog som er anvendt i distributionen. Egenskaben kan gentages for hver sprogvariant metadata forefindes på. Til angivelse af sprog skal følgende klassifikation anvendes: EU Vocabularies Languages Named Authority List, http://publications.europa.eu/resource/authority/language"@da ;
    rdfs:comment "A language of the resource."@en, "Ressourcens sprog."@da ;
    sh:pattern "^http://publications.europa.eu/resource/authority/language/" ;
    sh:flags "i" ;
    sh:path dct:language ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "checksum"@en, "checksum"@da ;
    sh:description "This property provides a mechanism that can be used to verify that the contents of a distribution have not changed. The checksum is related to the downloadURL"@en, "Denne egenskab angiver en værdi som gør det muligt at verificere at indholdet af en distribution ikke er blevet ændret. Checksum-værdien er relateret til download-url."@da ;
    rdfs:comment "The checksum property provides a mechanism that can be used to verify that the contents of a File or Package have not changed."@en, "Checksum-egenskaben gør det muligt at verificere at indholdet af en fil eller pakke ikke er blevet ændret.."@da ;
    sh:class <http://spdx.org/rdf/terms#Checksum> ;
    sh:maxCount 1 ;
    sh:path <http://spdx.org/rdf/terms#checksum> ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "byte size"@en, "bytestørrelse"@da ;
    sh:description "This property contains the size of a Distribution in bytes."@en, "Denne egenskab angiver størrelsen af en distribution angivet i bytes."@da ;
    rdfs:comment "The size of a distribution in bytes."@en, "Størrelsen af en distribution angivet i bytes."@da ;
    sh:datatype xsd:decimal ;
    sh:maxCount 1 ;
    sh:path dcat:byteSize ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "media type"@en, "medietype"@da ;
    sh:description "This property refers to the media type of the Distribution as defined in the official register of media types managed by IANA."@en, "Denne egenskab angiver medietypen for distributionen som den er defineret af Internet Assigned Numbers Authority (IANA) (https://www.iana.org/assignments/media-types/media-types.xhtml) "@da ;
    rdfs:comment "The media type of the distribution as defined by IANA."@en, "Medietypen for distributionen som den er defineret af IANA."@da ;
    sh:pattern "^https://www.iana.org/assignments/media-types/" ;
    sh:flags "i" ;
    sh:maxCount 1 ;
    sh:path dcat:mediaType ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "format"@en, "format"@da ;
    sh:description "This property refers to the file format of the Distribution"@en, "Denne egenskab angiver filformatet for distributionen som den er defineret af 'EU Vocabularies File Type Named Authority List' (http://publications.europa.eu/resource/authority/file-type)"@da ;
    rdfs:comment "The file format, physical medium, or dimensions of the resource."@en, "Ressourcens filformat, fysiske medium eller dimensioner."@da ;
    sh:pattern "^http://publications.europa.eu/resource/authority/file-type/" ;
    sh:flags "i" ;
    sh:maxCount 1 ;
    sh:path dct:format ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Recommended>
  ], [
    sh:name "compression format"@en, "kompressionsformat"@da ;
    sh:description "This property refers to the format of the file in which the data is contained in a compressed form, e.g. to reduce the size of the downloadable file. It SHOULD be expressed using a media type as defined in the official register of media types managed by IANA."@en, "Denne egenskab angiver filformatet for den distribution hvori data er indholdt i et komprimeret format, fx. med det formål at reducere størrelsen af downloadfilen. Formatet BØR udtrykkes med en medietype defineret af Internet Assigned Numbers Authority (IANA) (https://www.iana.org/assignments/media-types/media-types.xhtml) "@da ;
    rdfs:comment "The compression format of the distribution in which the data is contained in a compressed form, e.g. to reduce the size of the downloadable file."@en, "Kompressionsformatet for distributionen som indeholder data i et komprimeret format, fx. for at reducere størrelsen af downloadfilen,"@da ;
    sh:pattern "^https://www.iana.org/assignments/media-types/" ;
    sh:flags "i" ;
    sh:maxCount 1 ;
    sh:path dcat:compressFormat ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "packaging format"@en, "pakkeformat"@da ;
    sh:description "This property refers to the format of the file in which one or more data files are grouped together, e.g. to enable a set of related files to be downloaded together. It SHOULD be expressed using a media type as defined in the official register of media types managed by IANA."@en, "Denne egenskab angiver filformatet for den distribution som grupperer en eller flere datafiler, fx. med det formål at gøre det muligt at download et antal relaterede filer på én gang. Formatet BØR udtrykkes med en medietype defineret af Internet Assigned Numbers Authority (IANA) (https://www.iana.org/assignments/media-types/media-types.xhtml)"@da ;
    rdfs:comment "The package format of the distribution in which one or more data files are grouped together, e.g. to enable a set of related files to be downloaded together."@en, "Format til pakning af data med henblik på distribution af en eller flere relaterede datafiler der samles til en enhed med henblik på samlet distribution"@da ;
    sh:pattern "^https://www.iana.org/assignments/media-types/" ;
    sh:flags "i" ;
    sh:maxCount 1 ;
    sh:path dcat:packageFormat ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "documentation"@en, "dokumentation"@da ;
    sh:description "This property refers to a page or document about this Distribution."@en, "Denne egenskab angiver en webside eller et dokument som beskriver distibutionen."@da ;
    rdfs:comment "A page or document about this thing."@en, "En side eller et dokument vedrørende denne entitet."@da ;
    sh:class foaf:Document ;
    sh:path foaf:page ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "has policy"@en, "har politik"@da ;
    sh:description "This property refers to the policy expressing the rights associated with the distribution if using the ODRL vocabulary"@en, "Denne egenskab angiver de rettigheder der er forbundet med distributionen hvis W3Cs Open Digital Rights Language (ODRL) anvendes."@da ;
    rdfs:comment "Identifies an ODRL Policy for which the identified Asset is the target Asset to all the Rules."@en, "Identificerer en ODRL Politik hvis regler gælder det identificerede aktiv."@da ;
    sh:class odrl:Policy ;
    sh:maxCount 1 ;
    sh:path odrl:hasPolicy ;
    sh:severity sh:Violation ;
    sh:group <https://data.gov.dk/model/profile/dcat-ap-dk/Optional>
  ], [
    sh:name "license"@en, "licens"@da ;
    sh:description "This property refers to the licence under which the Distribution is made available."@en, "Denne egenskab angiver den licens distributionen er gjort tilgængelig under."@da ;
    rdfs:comment "A legal document giving official permission to do something with a resource."@en, "Et juridisk dokument der fastlægger de officielle tilladelser vedrørende en ressource."@da ;
